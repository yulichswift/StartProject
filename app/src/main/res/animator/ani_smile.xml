<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially">
    <objectAnimator
        android:duration="1000"
        android:propertyName="translateY"
        android:startOffset="0"
        android:valueFrom="0"
        android:valueTo="-10">
    </objectAnimator>
    <objectAnimator
        android:duration="1000"
        android:propertyName="translateY"
        android:startOffset="0"
        android:valueFrom="-10"
        android:valueTo="10">
    </objectAnimator>
    <objectAnimator
        android:duration="1000"
        android:propertyName="translateY"
        android:startOffset="0"
        android:valueFrom="10"
        android:valueTo="0">
    </objectAnimator>
    <objectAnimator
        android:duration="1000"
        android:propertyName="translateX"
        android:startOffset="0"
        android:valueFrom="0"
        android:valueTo="-10">
    </objectAnimator>
    <objectAnimator
        android:duration="1000"
        android:propertyName="translateX"
        android:startOffset="0"
        android:valueFrom="-10"
        android:valueTo="10">
    </objectAnimator>
    <objectAnimator
        android:duration="1000"
        android:propertyName="translateX"
        android:startOffset="0"
        android:valueFrom="10"
        android:valueTo="0">
    </objectAnimator>
</set>